function initHeaderSticky() {
  if (jQuery(document).height() > jQuery(window).height()) {
    if (jQuery(this).scrollTop() > 400) {
      jQuery('#go-top-btn').addClass("fixed");
    } else {
      jQuery('#go-top-btn').removeClass("fixed");
    }
  }
}
