function makeTimer() {
  // Get the current date and time
  var now = new Date();

  // Create a Date object for tomorrow at 11:30 AM
  var endTime = new Date();
  endTime.setDate(now.getDate() + 1); // Set to tomorrow
  endTime.setHours(11, 30, 0, 0); // Set time to 11:30:00

  // Convert times to seconds for calculation
  endTime = Math.floor(endTime.getTime() / 1000);
  now = Math.floor(now.getTime() / 1000);

  // Calculate the time left
  var timeLeft = endTime - now;
  var days = Math.floor(timeLeft / 86400);
  var hours = Math.floor((timeLeft - (days * 86400)) / 3600);
  var minutes = Math.floor((timeLeft - (days * 86400) - (hours * 3600)) / 60);
  var seconds = Math.floor((timeLeft - (days * 86400) - (hours * 3600) - (minutes * 60)));

  // Format hours, minutes, and seconds to always be two digits
  if (hours < 10) { hours = "0" + hours; }
  if (minutes < 10) { minutes = "0" + minutes; }
  if (seconds < 10) { seconds = "0" + seconds; }

  // Update the HTML with the remaining time
  $(".count-days").html(days);
  $(".count-hours").html(hours);
  $(".count-minites").html(minutes);
  $(".count-seconds").html(seconds);
}

// Call the function every second to update the countdown
setInterval(makeTimer, 100);


