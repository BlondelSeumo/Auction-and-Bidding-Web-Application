// Function to update the table based on the selected dropdown option
function setupdocupdateForm() {
  var selectElement = document.getElementById("product-name");
  var selectedValue = selectElement.value;
  var table = document.querySelector(".setup-login-details");
  var tableBody = document.getElementById("data-table-body");

  if (selectedValue in jsonData) {
    table.classList.remove("d-none");
    tableBody.innerHTML = ""; // Clear the table body
    if (Array.isArray(jsonData[selectedValue])) {
      // If it's an array, create a row for each set of data
      jsonData[selectedValue].forEach(function (item, index) {
        var row = tableBody.insertRow(index);
        var cell1 = row.insertCell(0);
        var cell2 = row.insertCell(1);

        cell1.innerHTML = item.Email;
        cell2.innerHTML = item.password;
      });
    } else {
      // If it's not an array, create a single row
      var row = tableBody.insertRow(0);
      var cell1 = row.insertCell(0);
      var cell2 = row.insertCell(1);

      cell1.innerHTML = jsonData[selectedValue].Email;
      cell2.innerHTML = jsonData[selectedValue].password;
    }
  } else {
    table.classList.add("d-none"); // Add the d-none class
  }
}
